{
	"scripts": {
		"build": "npm run babel && webpack --env production",
		"babel": "babel src -d functions/src && babel index.js -d functions",
		"babel:server": "babel -w index.js -d functions",
		"babel:src": "babel -w src -d functions/src",
		"browser": "browser-sync start --watch --files functions --port 3000 --proxy http://localhost:5000 --reload-delay 500",
		"browser:dev": "browser-sync dist --watch --single --port 3000",
		"dev": "rimraf dist/* && concurrently --kill-others \"webpack --config webpack.config.dev.js\" \"npm run browser:dev\"",
		"dev:emulated": "concurrently --kill-others \"npm run babel:src\" \"npm run babel:server\" \"webpack\" \"firebase emulators:start\" \"npm run browser\"",
		"postinstall": "cd functions && npm install"
	},
	"name": "chitragupta",
	"version": "1.0.0",
	"description": "An app for taking notes. (WIP)",
	"author": "ROG.stdioh",
	"private": true,
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.4",
		"@babel/core": "^7.15.5",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "^7.14.5",
		"autoprefixer": "^10.3.4",
		"babel-loader": "^8.2.2",
		"browser-sync": "^2.27.5",
		"concurrently": "^6.2.1",
		"copy-webpack-plugin": "^9.0.1",
		"css-loader": "^6.2.0",
		"cssnano": "^5.0.8",
		"mini-css-extract-plugin": "^2.3.0",
		"postcss": "^8.3.6",
		"postcss-loader": "^6.1.1",
		"rimraf": "^3.0.2",
		"sass": "^1.39.2",
		"sass-loader": "^12.1.0",
		"style-loader": "^3.2.1",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	}
}
